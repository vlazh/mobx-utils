{
  "name": "@vlazh/mobx-utils",
  "version": "2.9.2",
  "description": "MobX utils",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "VZH",
  "license": "MIT",
  "keywords": [
    "mobx",
    "mobx utils",
    "mobx store",
    "mobx model",
    "mobx serialization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/mobx-utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE .npmignore ./dist/",
    "minify": "node-utils-minify --replace ./dist",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '../../../tutoron/node_modules/@vlazh/mobx-utils/' && copyfiles package.json '../../../tutoron/node_modules/@vlazh/mobx-utils/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vlazh/mobx-utils/' && copyfiles package.json '../react-components/node_modules/@vlazh/mobx-utils/'",
    "build-to-player": "tsc --build ./tsconfig.json && npm run minify && copyfiles -e './dist/*(package.json|README.md|LICENSE)' -u 1 './dist/**/*' '../../../player/modules/mobx-utils'",
    "patch-publish": "npm run build && npm run minify && npm version patch && npm run copy:configs && npm publish ./dist/ && git push --follow-tags",
    "minor-publish": "npm run build && npm run minify && npm version minor && npm run copy:configs && npm publish ./dist/ && git push --follow-tags"
  },
  "devDependencies": {
    "@js-toolkit/configs": "^3.84.2",
    "@js-toolkit/node-utils": "^1.1.4",
    "@js-toolkit/utils": "^1.41.0",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jwt-decode": "^3.1.2",
    "localforage": "^1.10.0",
    "mobx": "^6.4.1",
    "mobx-utils": "^6.0.4",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "terser": "^5.11.0",
    "typescript": "^4.5.5",
    "valtors": "^2.4.1",
    "yargs": "^17.3.1"
  },
  "prettier": "@js-toolkit/configs/prettier"
}
